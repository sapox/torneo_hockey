<h1>Listing canchas</h1>

<table class="table">
  <thead>
  <tr>
    <th>Nombre</th>
    <th>Pais</th>
    <th>Provincia</th>
    <th>Ciudad</th>
    <th>Calle y numero</th>
    <th>Capacidad publico</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @canchas.each do |cancha| %>
  <tbody>
  <tr>
    <td><%= cancha.nombre %></td>
    <td><%= cancha.pais %></td>
    <td><%= cancha.provincia %></td>
    <td><%= cancha.ciudad %></td>
    <td><%= cancha.calle_y_numero %></td>
    <td><%= cancha.capacidad_publico %></td>
    <td><%= link_to 'Show', cancha %></td>
    <td><%= link_to 'Edit', edit_cancha_path(cancha) %></td>
    <td><%= link_to 'Destroy', cancha, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</tbody>
</thead>
</table>

<br />

<%= link_to 'New Cancha', new_cancha_path %>
