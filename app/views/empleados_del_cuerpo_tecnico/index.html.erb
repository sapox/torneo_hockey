<h1>Lista de empleados del cuerpo tecnico</h1>

<table class="table table-striped">
  <thead>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Dni</th>
    <th>Fecha de nacimiento</th>
    <th>Telefono fijo</th>
    <th>Telefono celular</th>
    <th>Email</th>
    <th>Direccion</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @empleados_del_cuerpo_tecnico.each do |empleado_del_cuerpo_tecnico| %>
  <tbody>
  <tr>
    <td><%= empleado_del_cuerpo_tecnico.nombre %></td>
    <td><%= empleado_del_cuerpo_tecnico.apellido %></td>
    <td><%= empleado_del_cuerpo_tecnico.dni %></td>
    <td><%= empleado_del_cuerpo_tecnico.fecha_de_nacimiento %></td>
    <td><%= empleado_del_cuerpo_tecnico.telefono_fijo %></td>
    <td><%= empleado_del_cuerpo_tecnico.telefono_celular %></td>
    <td><%= empleado_del_cuerpo_tecnico.email %></td>
    <td><%= empleado_del_cuerpo_tecnico.direccion %></td>
    <td><%= button_to 'Mostrar', empleado_del_cuerpo_tecnico, :method => "get" %></td>
    <td><%= button_to 'Editar', edit_empleado_del_cuerpo_tecnico_path(empleado_del_cuerpo_tecnico), :method => "get" %></td>
    <td><%= button_to 'Eliminar', empleado_del_cuerpo_tecnico, method: :delete, data: { confirm: 'Estas seguro?' }%></td>
  </tr>
<% end %>
</tbody>
</thead>
</table>

<br />

<div align="center">
<%= button_to 'Nuevo Empleado del cuerpo tecnico', new_empleado_del_cuerpo_tecnico_path, :method => "get" %>
</div>

